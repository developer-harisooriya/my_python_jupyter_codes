    "# While loop example\n",
    "counter = 0\n",
    "while counter < 6:\n",
    "    print(\"Counter:\", counter)\n",
    "    counter += 1\n",
    "\n",
    "# For loop example\n",
    "fruits = [\"apple\", \"banana\", \"cherry\"]\n",
    "for fruit in fruits:\n",
    "    print(\"Fruit:\", fruit)\n",
    "\n",
    "# Range-based loop example\n",
    "for i in range(1, 6):\n",
    "    print(\"Number:\", i)\n"
